session AutoCorres = CParser +
  options [timeout = 3600, quick_and_dirty]
  theories
    "AutoCorres"

session AutoCorresSEL4 = CBaseRefine +
  options [timeout = 7200, quick_and_dirty]
  theories
    "TestSEL4"

